{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-unreachable": "off", 
    "no-undef": "off",
    "no-console": "off",
    "no-debugger": "off",
    "no-alert": "off",
    "prefer-const": "off",
    "no-var": "off"
  }
}