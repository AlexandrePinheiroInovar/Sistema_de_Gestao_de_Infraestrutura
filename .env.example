# Configurações do Sistema MDU
GOOGLE_SHEETS_API_KEY=sua_api_key_aqui
SHEET_ID=id_da_planilha_google_sheets
REFRESH_INTERVAL=30000
DEBUG_MODE=false

# Configurações do Servidor
PORT=3000
HOST=localhost

# URLs de Desenvolvimento
DEV_SHEET_URL=https://docs.google.com/spreadsheets/d/SEU_ID_DESENVOLVIMENTO/edit
PROD_SHEET_URL=https://docs.google.com/spreadsheets/d/SEU_ID_PRODUCAO/edit

# Configurações de Cache
CACHE_DURATION=300000
ENABLE_AUTO_REFRESH=true

# Configurações de Logs
LOG_LEVEL=info
ENABLE_CONSOLE_LOGS=true